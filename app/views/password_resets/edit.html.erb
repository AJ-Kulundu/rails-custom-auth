<%= form_with model:@user, url: password_resets_path(token: params[:token]), class: "form" do |f| %>
    <div>
        <%=f.label :password%>
        <%= f.password_field :password %>
    <div>
    <div>
        <%=f.label :password_confirmation%>
        <%= f.password_field :password_confirmation %>
    <div>
    <div>
        <%= f.submit "Update Password" %>
    <div>
<%end%>